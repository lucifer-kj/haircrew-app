{
  "name": "haircrew-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:generate && next build",
    "build:skip-prisma": "next build",
    "build:windows": "npm run prisma:generate:windows && next build",
    "build:troubleshoot": "node scripts/build-troubleshoot.js",
    "prisma:generate": "prisma generate || echo 'Prisma generate failed, continuing with build...'",
    "prisma:generate:force": "rm -rf node_modules/.prisma && prisma generate",
    "prisma:generate:windows": "if exist node_modules\\.prisma rmdir /s /q node_modules\\.prisma && prisma generate || echo 'Windows Prisma generate failed, continuing...'",
    "start": "next start",
    "lint": "next lint",
    "seed": "tsx prisma/seed.ts",
    "postinstall": "npx lightningcss-cli --version || echo 'lightningcss optional'"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.4",
    "@motionone/utils": "^10.18.0",
    "@prisma/client": "^6.10.1",
    "@pusher/push-notifications-web": "^1.1.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@stagewise-plugins/react": "^0.5.1",
    "@stagewise/toolbar-next": "^0.5.1",
    "@tailwindcss/node": "^4.1.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io-client": "^1.4.36",
    "@uploadthing/react": "^7.3.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.523.0",
    "next": "^15.4.1",
    "next-auth": "^5.0.0-beta.29",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "resend": "^4.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.2.1",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "uploadthing": "^7.7.3",
    "vaul": "^1.1.2",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "framer-motion": "^12.23.3",
    "prisma": "^6.12.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
