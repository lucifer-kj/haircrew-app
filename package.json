{
  "name": "haircrew-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:generate && next build",
    "build:skip-prisma": "next build",
    "build:windows": "npm run prisma:generate:windows && next build",
    "build:troubleshoot": "node scripts/build-troubleshoot.js",
    "prisma:generate": "prisma generate || echo 'Prisma generate failed, continuing with build...'",
    "prisma:generate:force": "rm -rf node_modules/.prisma && prisma generate",
    "prisma:generate:windows": "if exist node_modules\\.prisma rmdir /s /q node_modules\\.prisma && prisma generate || echo 'Windows Prisma generate failed, continuing...'",
    "start": "next start",
    "lint": "next lint",
    "seed": "tsx prisma/seed.ts",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next/ node_modules/.cache/ && npm install",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.4",
    "@motionone/utils": "^10.18.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "@uploadthing/react": "^7.3.2",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.523.0",
    "next": "^15.4.1",
    "next-auth": "^4.24.7",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-swipeable": "^7.0.2",
    "recharts": "^3.1.0",
    "resend": "^4.6.0",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "uploadthing": "^7.7.3",
    "vaul": "^1.1.2",
    "zod": "^3.25.76",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "framer-motion": "^12.23.3",
    "npm-check-updates": "^16.14.17",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prisma": "^6.12.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
